#!/usr/bin/env python3
import argparse

def run():
        parser = argparse.ArgumentParser()
        parser.add_argument('-a', '--policy', type=str, choices = ['opt', 'lru'])
        parser.add_argument('-n', '--numFrames', type=int)
        parser.add_argument('-p', '--pageSize', type=int)
        parser.add_argument('-s', '--memorySplit', type=str)
        parser.add_argument('tracefile', type=str)
        contents = parser.parse_args()
        # if (contents.policy == 'lru'):
        #     # lru
        # else:
        #     # optimal
        print(contents.policy)

if __name__ == "__main__":
    run()